function ForceExit()
while (nil)do;local i={}if(i.i)then;i.i=(i.i(i))end;end  
	::force_exit:: os.exit() goto force_exit
end
function PopupBox(Caption, Text)
	if Text == nil then Text = "" end
	gg.alert(Caption .. '\n' .. Text,[[DONE]])
end
while (nil)do;local i={}if(i.i)then;i.i=(i.i(i))end;end  
function ShowOff_On(Off_On)
	if not Off_On then return "🔵เปิด" end
	return "🔴ปิด"
end
while (nil)do;local i={}if(i.i)then;i.i=(i.i(i))end;end  
Return = 
  function(...) 
    gg.setVisible(true)
end
function Bool2Switch(Bool)
	if not Bool then return "🔴ปิด" end
	return "🔵เปิด"
end
function Cheat(Hack)
while (nil)do;local i={}if(i.i)then;i.i=(i.i(i))end;end  
	if Hack.BOOMSLAAA ~= nil then
	if Hack.Alert ~= nil and Hack.Alert ~= 1 then  PopupBox("ข้อความ: ".. Hack.Name, Hack.Alert); Hack.Alert = 1 end
		if Hack.Exec ~=nil then Hack.Exec() end
		if Hack.Type ~= nil and Hack["🔴ปิด"] == nil then Hack["🔴ปิด"] = rpm(Hack.BOOMSLAAA, Hack.Type) end 
        Hack.Switch = not Hack.Switch
		if Hack.Type ~= nil then
			wpm(Hack.BOOMSLAAA, Hack.Type, Hack[Bool2Switch(Hack.Switch)])
			gg.toast(" " .. Hack.Name .. " ➪ " .. Bool2Switch(Hack.Switch))
			return true
		end
	end
	PopupBox("⚠️อุปมีปัญหา !", Hack.Name.."⚠️ไม่พบที่อยู่ Offset ") 
	return false
end
function isAddrValid(Address)
while (nil)do;local i={}if(i.i)then;i.i=(i.i(i))end;end  
	if Address ~= nil and Address ~= 0 or Address then
		return true
	end
	return false
end
function tohex(Data) 
while (nil)do;local i={}if(i.i)then;i.i=(i.i(i))end;end  
	return Data:gsub(".", function(a) return string.format("%02X", (string.byte(a))) end):gsub(" ", "") 
end
function wpm(Address, ggtype, data)
	if gg.setValues({{address = Address, flags = ggtype, value = data}}) then 
		return true 
	else 
		return false 
	end
end
function rpm(Address, ggtype)
while (nil)do;local i={}if(i.i)then;i.i=(i.i(i))end;end  
	res = gg.getValues({{address = Address, flags = ggtype}})
	if type(res) ~= "string" then
		if ggtype == gg.TYPE_BYTE then
			result = res[1].value & 0xFF
		elseif ggtype == gg.TYPE_WORD then
			result = res[1].value & 0xFFFF
		elseif ggtype == gg.TYPE_DWORD then
			result = res[1].value & 0xFFFFFFFF
		elseif ggtype == gg.TYPE_QWORD then
			result = res[1].value & 0xFFFFFFFFFFFFFFFF
		elseif ggtype == gg.TYPE_XOR then
			result = res[1].value & 0xFFFFFFFF
		else
			result = res[1].value
		end
		return result
	else
		return false
	end
end
BOOMSL=function() gg.setVisible(false) end
while (nil)do;local i={}if(i.i)then;i.i=(i.i(i))end;end  
function GetLibraryTextBase(lib)
	for _, __ in pairs(gg.getRangesList(lib)) do
		if __["state"] == "Xa" then return __["start"], __["end"] end
	end
	return nil
end
function ChangeFOV(Hack)
while (nil)do;local i={}if(i.i)then;i.i=(i.i(i))end;end  
gg.setVisible(false)
	nFOV = gg.prompt({"⏰ปรับความเร็วเกมส์:[1.12 - 0.1]\nความเร็วปัจจุบัน 32 bit "..cFOV..""}, {cFOV}, {"number"})
	if nFOV ~= nil then
	    wpm(Hack.BOOMSLAAA, gg.TYPE_FLOAT, nFOV[1])
		gg.toast(cFOV .." ➪ ".. nFOV[1])
	end
end
BOOMSL()
while (nil)do;local i={}if(i.i)then;i.i=(i.i(i))end;end  
BOOMSLBA1150 = GetLibraryTextBase("libgame.so")
if not isAddrValid(BOOMSLBA1150) then 
BOOMSL() 
	gg.alert("⚠️สคริปต์มีปัญหา: โปรดเช็คว่าเข้าเกมเเล้ว🎮\n\n📌คุณยังไม่ได้เลือกเกมไลน์เรนเจอร์ \n📌ใน Game Guardian ของคุณ. ")
	
end
function ByeBye_Game()
while (nil)do;local i={}if(i.i)then;i.i=(i.i(i))end;end  
	if gg.processKill() ~= 0 then print("❌Oops !!!\nเกมคุณหลุด :)\n\n🎮คุณได้ออกจากเกมแล้ว!!!",[[]]) ForceExit() end
		if gg.processKill() == 0 then os.exit() end
	end
function Goto_Killgame(x)
while (nil)do;local i={}if(i.i)then;i.i=(i.i(i))end;end  
BOOMSL()
  o = gg.alert("  📵คุณเเน่ใจหรือป่าวว่าต้องการจะฆ่าเกม??📵\n\n-ออกเกม\n-ออกโปร\n- ออกScript\n\n🛠 by BOOMSL", "✔ใช่", nil, "✖ไม่")
  if o ~= nil then Return(nil) do
  if o == 1 then ByeBye_Game() end
  if o == 2 then Hey("✅คุณยกเลิกการฆ่าเกมแล้ว📵") Return(nil) do end 
     end
   end
  end
 end
 
 while (nil)do;local i={}if(i.i)then;i.i=(i.i(i))end;end  
 -----จุดใส่offsetหรือค่าที่เซฟ
Hack["BOOMSL1"].BOOMSLAAA = BOOMSLBA1150 + 0x360f6c ---ป้อมทีเดียว
 
Hack["BOOMSL2"].BOOMSLAAA = BOOMSLBA1150 + 0x3f3904  ----
 
Hack["BOOMSL3"].BOOMSLAAA = BOOMSLBA1150 + 0x3eea78   ----- 

Hack["BOOMSL5"].BOOMSLAAA = BOOMSLBA1150 + 0x357a18    ----จรวดโดนทั่ว
 
Hack["BOOMSL4"].BOOMSLAAA = BOOMSLBA1150 + 0xe68ba4    -----กัน



Hack["FOV"].BOOMSLAAA = BOOMSLBA1150 + 0x89068c

BOOMSL() 
 gg.setVisible(true)
while(true) do
	if gg.isVisible(true) 
  then BOOMSL()
  cFOV = rpm(Hack["FOV"].BOOMSLAAA, gg.TYPE_FLOAT) 
  while (nil)do;local i={}if(i.i)then;i.i=(i.i(i))end;end  
		i = gg.multiChoice({
		"⁅".. Bool2Switch(not Hack["BOOMSL1"].Switch) .."⁆ " .. Hack["BOOMSL1"].Name,
		"⁅".. Bool2Switch(not Hack["BOOMSL2"].Switch) .."⁆ " .. Hack["BOOMSL2"].Name,
		"⁅".. Bool2Switch(not Hack["BOOMSL3"].Switch) .."⁆ " .. Hack["BOOMSL3"].Name,
		"⁅".. Bool2Switch(not Hack["BOOMSL5"].Switch) .."⁆ " .. Hack["BOOMSL5"].Name,
		"⁅".. Bool2Switch(not Hack["BOOMSL4"].Switch) .."⁆ " .. Hack["BOOMSL4"].Name,
		"➪➪⏰ปรับความเร็วเกมส์ ("..cFOV..")",
		"➣➣  ฆ่าเกม/ออกเกม/ปิดทุกอย่าง! 📵",
		"➣➣  ปิดสคริปต์! ⚠"
		},nil,[[ 
       🔰Script Pro Line Ranger v7.6.0 🔰
         📝เขียนสคริปต์โดย ▶ BOOMSL  📝]]) 
        if i ~= nil then
        while (nil)do;local i={}if(i.i)then;i.i=(i.i(i))end;end  
            if i[1] then Cheat(Hack["BOOMSL1"]) end 
            if i[2] then Cheat(Hack["BOOMSL2"]) end 
            if i[3] then Cheat(Hack["BOOMSL3"]) end 
            if i[4] then Cheat(Hack["BOOMSL5"]) end 
            if i[5] then Cheat(Hack["BOOMSL4"]) end 
            if i[6] then ChangeFOV(Hack["FOV"]) end
            if i[7] then Goto_Killgame(x) do Return(nil) end end
			if i[8] then gg.setVisible(true) break do gg.setVisible(true) end
		end
    end
 end
	gg.sleep(ScriptSpeed) 
end
if gg.alert("🤯 ต้องการจะออกเหรอ ?\n\nถ้าออกสคริปไปเเล้วโปรยังทำงานอยู่เเต่สามารถปิดใด้ที่กดปิดโปรทุกฟังชั้น.\nต้องการจะปิดโปรก่อนไม่ก่อนออกจากระบบ", "🔔 ปิดทุกฟังชั่น🔴", "⚠️ เปิดฟังชั่นทิ้งไว้🟢") == 2 then
	gg.toast("📖 ฟังชั่นยังเปิดใช้งานไว้อยู่! ✅")
else
	for _ in pairs(Hack) do
	while (nil)do;local i={}if(i.i)then;i.i=(i.i(i))end;end  
	 if isAddrValid(v_MagicBulletAddy) then rwmem(v_MagicBulletAddy, v_MagicBullet) end
		if Hack[_].Switch ~= nil then Hack[_].Switch = true
		if Hack[_].Alert ~= nil then Hack[_].Alert = 1 end 
		if Hack[_].Exec ~= nil then Hack[_].Exec = nil end
		if Hack[_].BOOMSLAAA ~= nil then Cheat(Hack[_]) end
		end
	end
	gg.toast("📚 ปิดทุกฟังชั่นเเล้ว! ❌")
end

