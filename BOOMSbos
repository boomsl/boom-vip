Hack["BOOMSL4"].BOOMSLAAA = BOOMSLBA1150 + 0x361f28 --แร่ไม่ลด

Hack["BOOMSL5"].BOOMSLAAA = BOOMSLBA1150 + 0x398310 --ตีไกล

Hack["BOOMSL6"].BOOMSLAAA = BOOMSLBA1150 + 0xe68ba4 --รีพอร์ต

Hack["BOOMSL7"].BOOMSLAAA = BOOMSLBA1150 + 0x3b1364 --ขีปนาวุธ 

Hack["BOOMSL8"].BOOMSLAAA = BOOMSLBA1150 + 0x3eea78 --แม่นยำ

Hack["FOV"].BOOMSLAAA = BOOMSLBA1150 + 0x89068c --เร่งความเรว

