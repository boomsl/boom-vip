function V1 ()
menu = gg.choice({ 
	"🔻เริ่มเมนูโปร🔻", 
	"📴ออกจากสคริปต์📴"
	}, nil,os.date" 🔰Script Pro Line Ranger v7.6.0 🔰                                                                                                                                                                                                                                            🎬📚  YOUTUBE :: BOOMSL  📚🎬") 
	if menu == nil then else
	if menu == 1 then test() end
	if menu == 2 then mb1() end
  end
  PGM = -1
 end
function mb1()
gg.toast("📄กำลังปิดโปรเเกรม...") 
os.exit() 
end 
function mb2()
os.exit() 
end 




function test()
menu = gg.choice({ 
	"🔵ปรับเอง🔴", 
	"👩‍💻ปรับอัตโนมัติ👩‍💻",
	"✴️โปรกิลด์เหรด✴️",
    "👑โปรเนียน PVP 👑",
    " 🏃‍♂️โปรสเตจวิ่ง🏃‍♀️",
    "🔄ย้อนกลับ🔄"
	}, nil,"🔻เลือกเมนูการปรับ🔻") 
	if menu == nil then else
	if menu == 1 then testA1() end
    if menu == 2 then testA2() end
    if menu == 3 then testA3() end
    if menu == 4 then testA4() end
    if menu == 5 then testA5() end
	if menu == 6 then V1() end
  end
  PGM = -1
 end
 
 
 




