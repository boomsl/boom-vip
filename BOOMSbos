end
function ByeBye_Game()
while (nil)do;local i={}if(i.i)then;i.i=(i.i(i))end;end  
	if gg.processKill() ~= 0 then print("❌Oops !!!\nเกมคุณหลุด :)\n\n🎮คุณได้ออกจากเกมแล้ว!!!",[[]]) ForceExit() end
		if gg.processKill() == 0 then os.exit() end
	end
function Goto_Killgame(x)
while (nil)do;local i={}if(i.i)then;i.i=(i.i(i))end;end  
BOOMSL()
  o = gg.alert("  📵คุณเเน่ใจหรือป่าวว่าต้องการจะฆ่าเกม??📵\n\n-ออกเกม\n-ออกโปร\n- ออกScript\n\n🛠 by BOOMSL", "✔ใช่", nil, "✖ไม่")
  if o ~= nil then Return(nil) do
  if o == 1 then ByeBye_Game() end
  if o == 2 then Hey("✅คุณยกเลิกการฆ่าเกมแล้ว📵") Return(nil) do end 
     end
   end
  end
 end
 
 while (nil)do;local i={}if(i.i)then;i.i=(i.i(i))end;end  
 -----จุดใส่offsetหรือค่าที่เซฟ
Hack["BOOMSL1"].BOOMSLAAA = BOOMSLBA1150 + 0x360f6c
 Hack["BOOMSL2"].BOOMSLAAA = BOOMSLBA1150 + 0x58a7b0
 Hack["BOOMSL3"].BOOMSLAAA = BOOMSLBA1150 + 0x89068c
 Hack["BOOMSL4"].BOOMSLAAA = BOOMSLBA1150 + 0x37be58
 Hack["FOV"].BOOMSLAAA = BOOMSLBA1150 + 0x000000

BOOMSL() 
 gg.setVisible(true)
while(true) do
	if gg.isVisible(true) 
  then BOOMSL()
  cFOV = rpm(Hack["FOV"].BOOMSLAAA, gg.TYPE_FLOAT) 
  while (nil)do;local i={}if(i.i)then;i.i=(i.i(i))end;end  
		i = gg.multiChoice({
		"⁅".. Bool2Switch(not Hack["BOOMSL1"].Switch) .."⁆ " .. Hack["BOOMSL1"].Name,
		"➣➣  ปิดสคริปต์! ⚠"
		},nil,[[ 
       Script LINE Rangers   📚 32 Bit 📚 
       ✏ เขียนโดย BOOMSL  V7.6.0  ]]) 
        if i ~= nil then
        while (nil)do;local i={}if(i.i)then;i.i=(i.i(i))end;end  
            if i[1] then Cheat(Hack["BOOMSL1"]) end 
            if i[1] then Cheat(Hack["BOOMSL2"]) end 
            if i[1] then Cheat(Hack["BOOMSL3"]) end 
            if i[1] then Cheat(Hack["BOOMSL4"]) end 
			if i[2] then gg.setVisible(true) break do gg.setVisible(true) end
		end
    end
 end
	gg.sleep(ScriptSpeed) 
end
if gg.alert("🤯 ต้องการจะออกเหรอ ?\n\nถ้าออกสคริปไปเเล้วโปรยังทำงานอยู่เเต่สามารถปิดใด้ที่กดปิดโปรทุกฟังชั้น.\nต้องการจะปิดโปรก่อนไม่ก่อนออกจากระบบ", "🔔 ปิดทุกฟังชั่น🔴", "⚠️ เปิดฟังชั่นทิ้งไว้🟢") == 2 then
	gg.toast("📖 ฟังชั่นยังเปิดใช้งานไว้อยู่! ✅")
else
	for _ in pairs(Hack) do
	while (nil)do;local i={}if(i.i)then;i.i=(i.i(i))end;end  
	 if isAddrValid(v_MagicBulletAddy) then rwmem(v_MagicBulletAddy, v_MagicBullet) end
		if Hack[_].Switch ~= nil then Hack[_].Switch = true
		if Hack[_].Alert ~= nil then Hack[_].Alert = 1 end 
		if Hack[_].Exec ~= nil then Hack[_].Exec = nil end
		if Hack[_].BOOMSLAAA ~= nil then Cheat(Hack[_]) end
		end
	end
	gg.toast("📚 ปิดทุกฟังชั่นเเล้ว! ❌")
end

